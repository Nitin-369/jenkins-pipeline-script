// hello All this is to show ypu what is declarative pipeline
// every declarative pipeline starts with pipeline

pipeline {
    agent any 
    stages {
        stage('Checkout') { 
            steps {
                echo "************Checkout*************" 
            }
        }
        stage('Build') { 
            steps {
                echo "************Build*************" 
            }
        }
        stage('Package') { 
            steps {
                echo "************Package*************" 
            }
        }
		stage('Deploy') { 
            steps {
                echo "************Deploy*************"
            }
        }
    }
}	
